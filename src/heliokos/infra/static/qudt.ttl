		] ,
		[
			a owl:Restriction ;
			owl:onProperty qudt:description ;
			owl:maxCardinality "1"^^xsd:int ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty qudt:rationale ;
			owl:minCardinality "0"^^xsd:nonNegativeInteger ;
