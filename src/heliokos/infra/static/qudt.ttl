	qudt:bytes "64"^^xsd:integer ;
	rdfs:isDefinedBy <http://qudt.org/2.1/schema/qudt> ;
	.

qudt:Encoding
	a owl:Class ;
	rdfs:subClassOf
		qudt:Concept ,
		[
			a owl:Restriction ;
			owl:onProperty qudt:bits ;
			owl:maxCardinality "1"^^xsd:integer ;
